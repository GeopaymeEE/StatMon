{ 
	"name" : "doctorjbeam/statmon",
	"version" : "1.0",
	"description" : "PHP-based server/website status monitor",
	"keywords" : [
		"status"
	],
	"homepage" : "http://status.whsc.vic.edu.au",
	"authors" : [
		{
			"name" : "Michael Greenhill",
			"email" : "greenhill.michael.a@edumail.vic.gov.au",
			"role" : "Developer"
		}
	],
	"config" : {
		"process-timeout" : "1200"
	},
	"repositories": [
		{
			"type" : "package",
			"package" : {
				"name" : "smarty/smarty",
				"version" : "3.1.9",
				"dist" : {
					"url" : "http://www.smarty.net/files/Smarty-3.1.9.zip",
					"type" : "zip"
				}
			}
		},
		{
			"type" : "composer",
			"url" : "https://packages.zendframework.com/"
		}
	],
	
	"require": {
		"php" : ">=5.5.0",
		"smarty/smarty" : ">=3.1.7",
		"zendframework/zend-stdlib" : "2.2.*",
		"zendframework/zend-db" : "2.2.*",
		"zendframework/zend-ldap" : "2.2.*",
		"zendframework/zend-http" : "2.2.*"
	}
}
